<?xml version="1.0"?>
<!DOCTYPE benchmark PUBLIC "+//IDN sosy-lab.org//DTD BenchExec benchmark 1.18//EN" "https://www.sosy-lab.org/benchexec/benchmark-1.18.dtd">
<benchmark tool="dartagnan" timelimit="5 min" hardtimelimit="5 min" memlimit="15 GB" cpuCores="4">

<require cpuModel="Intel Xeon E3-1230 v5 @ 3.40 GHz" cpuCores="8"/>

  <resultfiles>**.graphml</resultfiles>
  <option>assume</option>
      
<rundefinition name="TSO_unreach-call">
    <tasks name="locks">
      <option>tso</option>
      <option>cat/tso.cat</option>
      <includesfile>tso.set</includesfile>
      <propertyfile>properties/unreach-call.prp</propertyfile>
    </tasks>
</rundefinition>

<rundefinition name="ARM_unreach-call">
    <tasks name="locks">
      <option>arm8</option>
      <option>cat/aarch64.cat</option>
      <includesfile>arm.set</includesfile>
      <propertyfile>properties/unreach-call.prp</propertyfile>
    </tasks>
</rundefinition>

<rundefinition name="POWER_unreach-call">
    <tasks name="locks">
      <option>power</option>
      <option>cat/power.cat</option>
      <includesfile>power.set</includesfile>
      <propertyfile>properties/unreach-call.prp</propertyfile>
    </tasks>
</rundefinition>

</benchmark>

